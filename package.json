{
  "name": "trek",
  "version": "0.0.0",
  "description": "Fast Async Web Framework For Modern Node.js",
  "homepage": "https://trekjs.com/",
  "repository": "trekjs/trek",
  "author": {
    "name": "Fangdun Cai",
    "email": "cfddream@gmail.com",
    "url": "fundon.me"
  },
  "license": "MIT",
  "keywords": [
    "async",
    "async/await",
    "framework",
    "middleware",
    "mvc",
    "rest",
    "server",
    "trek",
    "web"
  ],
  "main": "lib/index.js",
  "scripts": {
    "bench": "cd benchmarks; make all",
    "clean": "rm -rf coverage .nyc_output",
    "lint": "xo",
    "test": "xo && nyc ava",
    "test:watch": "xo && nyc ava"
  },
  "dependencies": {
    "dotenv": "4.0.0",
    "globby": "6.1.0",
    "interop-require": "1.0.0",
    "lodash.get": "4.4.2",
    "lodash.has": "4.5.2",
    "lodash.set": "4.3.2",
    "pino": "4.5.3",
    "trek-engine": "1.0.1",
    "trek-middleware": "1.1.0",
    "trek-router": "1.2.0",
    "trek-sessions": "0.1.6"
  },
  "devDependencies": {
    "ava": "0.19",
    "babel-eslint": "7",
    "eslint-config-trek": "1",
    "nyc": "10",
    "parseurl": "1",
    "request": "2",
    "request-promise": "4",
    "xo": "0.18"
  },
  "engines": {
    "node": ">=7.7"
  },
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "dist",
    "lib"
  ],
  "ava": {
    "concurrency": 5
  },
  "nyc": {
    "reporter": [
      "html",
      "lcov",
      "text"
    ]
  },
  "xo": {
    "esnext": true,
    "semicolon": false,
    "space": true,
    "extends": "trek",
    "parser": "babel-eslint"
  }
}
